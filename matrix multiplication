#include<stdio.h>
int main()
{
    int i,j,k,n=2,m=2,c[2][2];
    int a[2][2]={1,2,3,4};
    int b[2][2]={5,6,7,8};
    for(i=0;i<n;i++)
    {
        for(j=0;j<m;j++)
        {
            for(k=0;k<2;k++)
            {
                c[i][j]+=a[i][k]*b[j][k];
            }
            printf("%d\n",c[i][j]);
        }
    }
    return 0;
}

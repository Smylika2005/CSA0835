#include<stdio.h>
int main()
{
    int i,j,a[5]={1,3,6,2,5},temp,n=5;
    for(i=0;i<=n;i++)
    {
        for(j=i+1;j<=n;j++)
        {
            if(a[i]<a[j])
            {
            temp=a[i];
            a[i]=a[j];
            a[j]=temp;
printf("%d",a[i]);
            }
        }
        
    }
return 0;
}
